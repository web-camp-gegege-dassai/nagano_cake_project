<%= form_with model: address, local:true  do |f| %>
  <div class="form-group">
    <%= f.label :郵便番号 %>
    <%= f.text_field :postal_code %>
  </div>
  <div class="form-group">
    <%= f.label :住所 %>
    <%= f.text_field :address %>
  </div>
  <div class="form-group">
    <%= f.label :宛名 %>
    <%= f.text_field :postal_code%>
  </div>
  <%= f.submit "btn" %>
<% end %>