 <div class="account-page-title">
    <h1>会員編集画面</h1>
　</div>
  
 <%= form_with model:@user, local:true do |f| %>
        <div class="form-group">
          <label for="inputName">氏名</label>
          <p><%= f.text_field :last_name, autofocus: true, id:"inputName", placeholder:"姓"%></p>
          <p><%= f.text_field :first_name, placeholder:"名"%></p>
        </div>
        
        <div class="form-group">
          <label for="inputName">フリガナ</label>
          <p><%= f.text_field :last_name_kana, placeholder:"姓カナ"%></p>
          <p><%= f.text_field :first_name_kana, placeholder:"名カナ"%></p>
        </div>
        
        <div class="form-group">
          <label for="inputName">郵便番号</label>
          <p><%= f.text_field :postal_code, placeholder:"郵便番号"%></p>
        </div>
        
        <div class="form-group">
          <label for="inputName">住所</label>
          <p><%= f.text_field :address, placeholder:"住所"%></p>
        </div>
        
         <div class="form-group">
          <label for="inputName">電話番号</label>
          <p><%= f.text_field :telephone_number, placeholder:"電話番号"%></p>
        </div>
        
         <div class="form-group">
          <label for="inputName">メールアドレス</label>
          <p><%= f.email_field :email, placeholder:"メールアドレス"%></p>
        </div>
        
        
        
        <%= f.submit "編集内容を保存" %>
      <% end %>
    </div>